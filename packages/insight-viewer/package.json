{
  "name": "@lunit/insight-viewer",
  "version": "5.0.1-alpha.1",
  "description": "cornerstone 라이브러리를 기반으로 Dicom Image를 다루기 위한 여러 Component들을 제공한다.",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lunit-io/frontend-components"
  },
  "keywords": [
    "dicom",
    "dcm",
    "cornerstone",
    "react"
  ],
  "license": "MIT",
  "homepage": "https://github.com/lunit-io/frontend-components#readme",
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "dev": "microbundle watch -f esm --jsx React.createElement --tsconfig tsconfig.json",
    "compile": "microbundle --jsx React.createElement --tsconfig tsconfig.build.json",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "clean:doc": "rm -rf ../../out/handbook/src/insight-viewer",
    "test": "yarn jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ../../out/handbook/src/insight-viewer --quiet",
    "doc": "yarn run build-storybook"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@storybook/addon-actions": "^6.2.5",
    "cornerstone-core": "2.3.0",
    "cornerstone-wado-image-loader": "3.3.1",
    "cornerstone-web-image-loader": "2.1.1",
    "dicom-parser": "1.8.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-docs": "^6.2.3",
    "@storybook/addon-essentials": "^6.2.3",
    "@storybook/addon-links": "^6.2.3",
    "@storybook/react": "^6.2.3",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "babel-loader": "^8.2.2",
    "consola": "^2.15.3",
    "eslint-import-resolver-node": "^0.3.4",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "react-is": "^17.0.2",
    "ts-jest": "^26.5.4"
  }
}
